#!/bin/ksh

#############################################################################
# This section is some parameter setup for for development testing only
##############################################################################
echo `date` $0 test section begin
export fhmax=384
export fhmaxh=384
export FHOUTHR=6
export VERBOSE=no
export navg_min=3

#define tmp time step
export DELTIM=300
export k_split=1
export n_split=8
export TYPE=nh
export MONO=non-mono
export LOGNAME=emc.enspara
export SUBNAME=/$First.$Last
export PROJECT=ensemble
export JOBPRIO=devhigh
export SAVE=/gpfs/hps3/emc/$PROJECT/save/$LOGNAME$SUBNAME
export NOSCRUB=/gpfs/hps3/emc/$PROJECT/$LOGNAME$SUBNAME/noscrub
export basesource=$SAVE/$expid
export FNTSFA=/gpfs/hps3/emc/ensemble/noscrub/Wei.Li/CFS_ANOM2_create_CFSR_0p25/sst/outdata_1999/TMPsfc.$PDY$cyc.24hr.anom.grb
export FTSFS=0
export SOURCEDIR=/gpfs/hps3/emc/ensemble/save/emc.enspara/Hong.Guan/&EXPID;/nwdev
export nstf_name=2,1,5,0,0
export ivegsrc=1
export isot=1
export FCSTEXEC=fv3_gfs_nh.prod.32bit.x.tf

# cpu geometry
export layout_x=4
export layout_y=8
export WRITE_GROUP=2
export WRTTASK_PER_GROUP=24
export parallel_threads=2

#Define using STTP(ENS_SPS=.true.) or physics stochastic
export ENS_SPS=.false.
export DO_SPPT=YES
export DO_SHUM=YES
export DO_SKEB=YES

# for test, NOT USED FOR PRODUCTION gfssource = dev, para, prod
export gfssource=prod

# set all the following "make" and "save" flags to "yes" to simulate production
export makepgrba=no
export makepgrbb=no
export makepgrb2b=yes
export saveflux=yes
export savesfcsig=no
export sigzvd=no
export npair=2
export npert=4

echo `date` $0 test section end
