#!/bin/ksh

#############################################################################
# This section is some parameter setup for for development testing only
##############################################################################
echo `date` $0 test section begin
export fhmax=384
export fhmaxh=384
export FHOUTHR=6
export VERBOSE=no

#define tmp time step
export DELTIM=450
export k_split=2
export n_split=6
export TYPE=nh
export MONO=non-mono

# cpu geometry
export layout_x=4
export layout_y=8
export WRITE_GROUP=2
export WRTTASK_PER_GROUP=24
export parallel_threads=2

#Define using STTP(ENS_SPS=.true.) or physics stochastic
export ENS_SPS=.false.
export DO_SPPT=YES
export DO_SHUM=YES
export DO_SKEB=YES
export warm_start=.false.

# for test, NOT USED FOR PRODUCTION gfssource = dev, para, prod
export gfssource=para
export imp_physics=11   #default 11 for gfdl mp; 99 for ZhaoCarr; 6 for WSM6; 8 for Thompson

# set all the following "make" and "save" flags to "yes" to simulate production
export makepgrba=no
export makepgrbb=no
export makepgrb2b=no
export saveflux=yes
export savesfcsig=no
export sigzvd=no
export npair=10
export npert=20

echo `date` $0 test section end
