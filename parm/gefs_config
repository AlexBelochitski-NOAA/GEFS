  export HOMEtrak=$HOMEDIR/ens_tracker.v1.0.0
  export HOMEgene=$HOMEDIR/ens_genesis.v1.0.0

  export SENDCOM=YES
  export SENDDBN=NO
  export SENDDBN_GB2=NO
  export SENDDBN_NTC=NO
  export SENDECF=NO
  export SENDSMS=NO

#NCO required variable
  export UTILROOT=/nwprod/util
  export HOMEutil=$UTILROOT

  export NWROOT=/nw$envir 
  export NET=gens
  export RUN=$RUN
  export RUNMEM=$RUNMEM

  export DATAROOT=$basetmp/tmpnwprd
  export DATA=
  export DCOMROOT=
  export PCOMROOT=
  export COMROOT=$baseoutput
  export COM=$COMROOT/com/gens/dev
  export GESROOT=$COMROOT/nwges
  export jlogfile=$COMROOT/com/logs/jlogfile

  export DBNROOT=

#Our variable, may be modified to use the above NOC requested
  export DATA_IN=$basetmp/tmpnwprd
  export jlogfile=$DATA_IN/jlogfile
  export jlogfile=$baselog/com/logs/jlogfile

  export com=/ptmp/$LOGNAME/com/gens/dev
  export com=$baseoutput/com/gens/dev

  export COM=/ptmp/$LOGNAME/com/gens/dev
  export COM=$baseoutput/com/gens/dev

  export COM_IN=/ptmp/$LOGNAME/com/gens/dev
  export COM_IN=$baseoutput/com/gens/dev

  export COM_OUT=/ptmp/$LOGNAME/com/gens/dev
  export COM_OUT=$baseoutput/com/gens/dev

  export gespath=/ptmp/$LOGNAME/nwges/dev
  export gespath=$baseoutput/nwges/dev
  export gespath=$GESROOT/dev

  export COMGFS=/com/gfs/prod
# export COMGFS=$basedgfs/com/gfs/prod

  export pcom_in=/ptmp/$LOGNAME/pcom
  export pcom_in=$baseoutput/pcom

#Temperary setting of some variables, please check here against J-jobs carefully
# The cnvgrib utility executable, we used a temporay version cwin developmentn
# It is implemented in nwprod in Jan. 2015
  export CNVGRIB=/nco/sib/gribdev/util/exec/cnvgrib21_gfs
  export CNVGRIB=/nwprod/util/exec/cnvgrib21_gfs
# For JGEFS_CQPF
  export COMINGEST=${COMINGEST:-/ensemble/noscrub/Yan.Luo/com-ingest-prod}
# export COM_IN=/ensemble/noscrub/emc.enspara/xz/T574L64SL/EK0
