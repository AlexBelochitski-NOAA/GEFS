<metatask>
   <var name="member">&MEMLIST;</var>
      <task name="jgefs_init_fv3chgrs_#member#" cycledefs="gefs" maxtries="1">
      <jobname><cyclestr>&EXPID;@Y@m@d@H15_#member#</cyclestr></jobname>
      <account>&ACCOUNT;</account>
      <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_#member#_init_fv3chgrs@H.$PBS_JOBID</cyclestr></join>
      <walltime>00:30:00</walltime>
      <queue>&CUE2RUN;</queue>
      <nodes>1:ppn=12:tpp=2</nodes>
      
      <memory>50000M</memory>
      

      &ENV_VARS;
      &DATE_VARS;
      <envar>
         <name>SOURCEDIR</name>
         <value>&SOURCEDIR;</value>
      </envar>
      <envar>
         <name>job</name>
         <value><cyclestr>&EXPID;@Y@m@d@H15#member#</cyclestr></value>
      </envar>
      <envar>
         <name>RUNMEM</name>
         <value>ge#member#</value>
      </envar>
      <command><cyclestr>&PRE; &BIN;/jgefs_init_fv3chgrs.sh</cyclestr></command>
      <dependency>
         <taskdep task="jgefs_init_combine"/>
      </dependency>

      </task>
</metatask>
