<task name="jgefs_cleanup" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@y@m@d@H_clean</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_cleanup_@H.$PBS_JOBID</cyclestr></join>
   <walltime>00:30:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>1:ppn=1:tpp=1</nodes>
   
   <memory>3000M</memory>   

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@y@m@d@H_save</cyclestr></value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_cleanup.py</cyclestr></command>
   <dependency>
      <and>
         <taskdep task="jgefs_keep_data"/>
         <taskdep task="jgefs_archive"/>
      </and>
   </dependency>
</task>