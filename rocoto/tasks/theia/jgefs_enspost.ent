<task name="jgefs_enspost" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@Y@m@d@H395</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_enspost_@H.$PBS_JOBID</cyclestr></join>
   <walltime>01:29:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>1:ppn=1:tpp=24</nodes>
   <native>-cwd &tmpnwprd;</native>
   <memory>3000M</memory>
   <native>-extsched "CRAYLINUX[]"</native>

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@Y@m@d@H305</cyclestr></value>
   </envar>
   <envar>
      <name>RUNMEM</name>
      <value>#member#</value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_enspost.sh</cyclestr></command>
   <dependency>
      <taskdep task="jgefs_ensstat_high"/>
   </dependency>
</task>
