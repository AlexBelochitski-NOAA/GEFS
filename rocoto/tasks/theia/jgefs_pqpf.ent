<task name="jgefs_pqpf" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@Y@m@d@H308</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_pqpf_@H.$PBS_JOBID</cyclestr></join>
   <walltime>01:30:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>1:ppn=16</nodes>
   
   <memory>500M</memory>
   

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@Y@m@d@H308</cyclestr></value>
   </envar>
   <envar>
      <name>RUNMEM</name>
      <value>#member#</value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_pqpf.sh</cyclestr></command>
   <dependency>
      <taskdep task="jgefs_enspost"/>
   </dependency>
</task>
