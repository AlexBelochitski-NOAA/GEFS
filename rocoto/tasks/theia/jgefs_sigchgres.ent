<metatask name="jgefs_sigchgres" mode="parallel">
   <var name="member">p01 p02 p03 p04 p05 p06 p07 p08 p09 p10 p11 p12 p13 p14 p15 p16 p17 p18 p19 p20 c00</var>
   <task name="jgefs_sigchgres_#member#" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@Y@m@d@H200_#member#</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_#member#_sigchgres_@H.$PBS_JOBID</cyclestr></join>
   <walltime>00:20:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>1:ppn=1:tpp=4</nodes>
   
   <memory>3000M</memory>
   

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@Y@m@d@H200#member#</cyclestr></value>
   </envar>
   <envar>
      <name>RUNMEM</name>
      <value>ge#member#</value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_sigchgres.sh</cyclestr></command>
   <dependency>
      <taskdep task="jgefs_ensstat_high"/>
   </dependency>
   </task>
</metatask>
