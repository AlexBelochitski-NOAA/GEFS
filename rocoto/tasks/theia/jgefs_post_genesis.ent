<task name="jgefs_post_genesis" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@Y@m@d@H397</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_post_genesis_@H.$PBS_JOBID</cyclestr></join>
   <walltime>00:45:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>3:ppn=7</nodes>
   
   <!-- <memory>3000M</memory> -->
   

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@Y@m@d@H307</cyclestr></value>
   </envar>
   <envar>
      <name>RUNMEM</name>
      <value>#member#</value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_post_genesis.sh</cyclestr></command>
   <dependency>
      <metataskdep metatask="jgefs_prdgen_high"/>
   </dependency>
</task>
