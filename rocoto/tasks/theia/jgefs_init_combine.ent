<task name="jgefs_init_combine" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@Y@m@d@H014</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_init_combine_@H.$PBS_JOBID</cyclestr></join>
   <walltime>01:50:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>20:ppn=2</nodes>
   
   <memory>30000M</memory>
   

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@Y@m@d@H014</cyclestr></value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_init_combine.sh</cyclestr></command>
   <dependency>
      <taskdep task="jgefs_init_process"/>
</dependency>
</task>
