<metatask>
   <var name="member">&MEMLIST;</var>
      <task name="jgefs_forecast_high1_#member#" cycledefs="gefs" maxtries="1">
      <jobname><cyclestr>&EXPID;@Y@m@d@H100_#member#</cyclestr></jobname>
      <account>&ACCOUNT;</account>
      <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_#member#_forecast_high_@H.$PBS_JOBID</cyclestr></join>
      <walltime>03:00:00</walltime>
      <queue>&CUE2RUN;</queue>
      <nodes>18:ppn=12:tpp=2</nodes>
      
      <memory>60000M</memory>
      
      &ENV_VARS;
      &DATE_VARS;
      <envar>
         <name>SOURCEDIR</name>
         <value>&SOURCEDIR;</value>
      </envar>
      <envar>
         <name>job</name>
         <value><cyclestr>&EXPID;@Y@m@d@H100#member#</cyclestr></value>
      </envar>
      <envar>
         <name>RUNMEM</name>
         <value>ge#member#</value>
      </envar>
      <command><cyclestr>&PRE; &BIN;/jgefs_forecast_high.sh</cyclestr></command>
      <dependency>
         <taskdep task="jgefs_init_fv3chgrs_#member#"/>
      </dependency>

      </task>
</metatask>
