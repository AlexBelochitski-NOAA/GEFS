<task name="jgefs_archive" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@y@m@d@H_arch</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_archive_@H.%J</cyclestr></join>
   <walltime>03:00:00</walltime>
   <queue>&TRANSFER_QUEUE;</queue>
   <nodes>1:ppn=1:tpp=1</nodes>   
   <native>-cwd &tmpnwprd;</native>
   <memory>3000M</memory>   
   <native>-extsched "CRAYLINUX[]"</native>

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@y@m@d@H_arch</cyclestr></value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_archive.py</cyclestr></command>
   <dependency>
      <taskdep task="jgefs_enspost"/>
   </dependency>
</task>