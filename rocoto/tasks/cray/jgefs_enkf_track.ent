<task name="jgefs_enkf_track" cycledefs="gefs" maxtries="1">
   <jobname><cyclestr>&EXPID;@Y@m@d@H010</cyclestr></jobname>
   <account>&ACCOUNT;</account>
   <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_enkf_track_@H.%J</cyclestr></join>
   <walltime>01:50:00</walltime>
   <queue>&CUE2RUN;</queue>
   <nodes>2:ppn=20</nodes>
   <native>-cwd &tmpnwprd;</native>
   <memory>3000M</memory>
   <native>-extsched "CRAYLINUX[]"</native>

   &ENV_VARS;
   &DATE_VARS;
   <envar>
      <name>SOURCEDIR</name>
      <value>&SOURCEDIR;</value>
   </envar>
   <envar>
      <name>WORKDIR</name>
      <value>&WORKDIR;</value>
   </envar>
   <envar>
      <name>job</name>
      <value><cyclestr>&EXPID;@Y@m@d@H010</cyclestr></value>
   </envar>
   <command><cyclestr>&PRE; &BIN;/jgefs_enkf_track.sh</cyclestr></command>
</task>
