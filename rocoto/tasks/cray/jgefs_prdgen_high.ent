<metatask name="jgefs_prdgen_high" mode="parallel">
   <var name="member">&MEMLIST;</var>
   <task name="jgefs_prdgen_high_#member#" cycledefs="gefs" maxtries="1">
      <jobname><cyclestr>&EXPID;@Y@m@d@H103_#member#</cyclestr></jobname>
      <account>&ACCOUNT;</account>
      <join><cyclestr>&LOG_DIR;/@Y@m@d/gefs_#member#_prdgen_high_@H.%J</cyclestr></join>
      <walltime>01:30:00</walltime>
      <queue>&CUE2RUN;</queue>
      <nodes>1:ppn=5:tpp=4</nodes>
      <native>-cwd &tmpnwprd;</native>
      <memory>3000M</memory>
      <native>-extsched "CRAYLINUX[]"</native>

      &ENV_VARS;
      &DATE_VARS;
      <envar>
         <name>SOURCEDIR</name>
         <value>&SOURCEDIR;</value>
      </envar>
      <envar>
         <name>job</name>
         <value><cyclestr>&EXPID;@Y@m@d@H103#member#</cyclestr></value>
      </envar>
      <envar>
         <name>RUNMEM</name>
         <value>ge#member#</value>
      </envar>
      <command><cyclestr>&PRE; &BIN;/jgefs_prdgen_high.sh</cyclestr></command>

      <dependency>
         <datadep><cyclestr>&DATA_DIR;/gefs.@Y@m@d/@H/misc/post/ge#member#.t@Hz.master.control.f00</cyclestr></datadep>
      </dependency>
   </task>
</metatask>
