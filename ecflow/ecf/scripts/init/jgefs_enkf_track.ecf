export MP_EUILIB=us
export MP_SHARED_MEMORY=yes
export MEMORY_AFFINITY=core:7 

export total_tasks=40
export taskspernode=4      
export NODES=10

# JY export total_tasks=40


export cyc=%CYC%
export cyc_fcst=%CYC%

# CALL executable job script here

. /usrx/local/prod/lmod/lmod/init/ksh
