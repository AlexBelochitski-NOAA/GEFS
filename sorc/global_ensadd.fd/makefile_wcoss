SHELL=  /bin/bash
CMD=	global_ensadd
#FC=     ncepxlf
FC=     ifort
#FOPTS=  -qsmp=noauto
FFLAGS=  -list -traceback
LDFLAGS=
INCS=
SRCS=	ENSADD.f 
OBJS=   $(SRCS:.f=.o)
#LIBS    = -l essl -L/nwprod/lib -lbacio_4 -lw3_4 -lip_4 -lsp_4
LIBS    = -L/nwprod/lib -lbacio_v2.0.1_4 -lw3nco_v2.0.6_4

#$(CMD): $(SRCS)
#	$(FC) $(FOPTS) $(SRCS) $(LIBS) -o $(CMD)
#global_ensadd: $(SRCS)
#	$(FC) $(FOPTS) $(SRCS) $(LIBS) -o ensadd

# Lines from here on down should not need to be changed.  They are the
# actual rules which make uses to build a.out.
#
all:    $(CMD)

$(CMD): $(OBJS)
	$(FC) $(LDFLAGS) $(FFLAGS) -o $(@) $(OBJS) $(LIBS)

$(OBJS):        $(SRCS)
	$(FC) $(FFLAGS) $(INCS) -c $(SRCS)

$(OBJS):        makefile_wcoss

allclean:       clean all

clean:
	-rm -f $(OBJS)

clobber:        clean
	-rm -f $(CMD)
